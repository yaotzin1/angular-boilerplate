{
    "version": "1.5.0",
    "compilerOptions": {
        "target": "es5",
        "module": "system",
        "moduleResolution": "node",
        "sourceMap": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false,
        "outDir": "./app",
        "rootDir": "./app_ts"
    },
    "exclude": [
        "node_modules",
        "typings"
    ]
}
